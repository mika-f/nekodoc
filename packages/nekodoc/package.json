{
  "name": "nekodoc",
  "version": "1.0.0",
  "description": "NekoDoc is a static site generator.",
  "repository": "https://github.com/mika-f/nekodoc",
  "author": "Natsune Mochizuki",
  "license": "MIT",
  "private": false,
  "type": "module",
  "main": "./src/lib/index.js",
  "types": "./src/lib/index.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./src/lib/index.ts",
        "default": "./src/lib/index.js"
      }
    }
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "nekodoc": "node --loader ts-node/esm bin/nekodoc.ts"
  },
  "bin": {
    "nekodoc": "./bin/nekodoc.js"
  },
  "dependencies": {
    "@mdx-js/mdx": "^2.1.2",
    "@nekodoc/logger": "*",
    "autoprefixer": "^10.4.8",
    "chokidar": "^3.5.3",
    "commander": "^9.4.0",
    "esbuild": "^0.15.5",
    "esbuild-style-plugin": "^1.6.0",
    "express": "^4.18.1",
    "get-port": "^6.1.2",
    "import-fresh": "^3.3.0",
    "js-yaml": "^4.1.0",
    "mkdirp": "^1.0.4",
    "postcss": "^8.4.16",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "readline": "^1.3.0",
    "remark-frontmatter": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "tailwindcss": "^3.1.8",
    "tmp": "^0.2.1"
  },
  "devDependencies": {
    "@types/chokidar": "^2.1.3",
    "@types/express": "^4.17.13",
    "@types/js-yaml": "^4.0.5",
    "@types/mkdirp": "^1.0.2",
    "@types/node": "^18.7.6",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/tmp": "^0.2.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
